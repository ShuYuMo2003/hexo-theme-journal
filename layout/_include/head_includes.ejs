<%- include("cdn.ejs") %>
<link rel="stylesheet" href="<%= this.cdn.libs.css.bootstrap %>" crossorigin="anonymous">
<link rel="stylesheet" href="<%- url_for(`css/journal.css?${Math.round(Math.random() * 99999999)}`) %>">
<% if (theme.favicon || config.favicon){ %>
<link rel="icon" href="<%- config.favicon ? config.favicon : theme.favicon %>">
<% } %>
<% if (theme.hljsTheme) { %>
<link rel="stylesheet" href="<%= ["/", "http:", "https:"].some(s => theme.hljsTheme.startsWith(s)) ? theme.hljsTheme : (this.cdn.libs.css.hljs + theme.hljsTheme + this.cdn.libs.css.hljsSuffix) %>" crossorigin="anonymous">
<% } %>
<link rel="stylesheet" href="<%= this.cdn.fonts %>/css?family=Noto+Serif|Noto+Serif+SC:400|Fira+Code|Montserrat|Material+Icons">
