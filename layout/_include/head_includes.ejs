<%- include("cdn.ejs") %>
<link rel="stylesheet" href="<%= this.cdn.libs.css.bootstrap %>" crossorigin="anonymous">
<link rel="stylesheet" href="<%- url_for(`css/journal.css?${Math.round(Math.random() * 99999999)}`) %>">
<% if (theme.favicon || config.favicon){ %>
<link rel="icon" href="<%- config.favicon ? config.favicon : theme.favicon %>">
<% } %>
<% if (theme.hljsTheme) { %>
<link id="hljsThemeLight" rel="stylesheet" href="<%= ["/", "http:", "https:"].some(s => theme.hljsTheme.light.startsWith(s)) ? theme.hljsTheme.light : (this.cdn.libs.css.hljs + theme.hljsTheme.light + this.cdn.libs.css.hljsSuffix) %>" crossorigin="anonymous">
<link disabled id="hljsThemeDark" rel="stylesheet" href="<%= ["/", "http:", "https:"].some(s => theme.hljsTheme.dark.startsWith(s)) ? theme.hljsTheme.dark : (this.cdn.libs.css.hljs + theme.hljsTheme.dark + this.cdn.libs.css.hljsSuffix) %>" crossorigin="anonymous">
<% } %>
<link rel="stylesheet" href="<%= this.cdn.fonts %>/css?family=Noto+Serif|Noto+Serif+SC|Fira+Code|Montserrat">
<link rel="stylesheet" href="<%= this.cdn.fonts %>/icon?family=Material+Icons">